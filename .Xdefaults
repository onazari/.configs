! --- special colors ---

URxvt*background: #2b3845
URxvt*foreground: #d9e6f2

! --- standard colors ---

! black
*color0: #0d1926

! bright_black
*color8: #2b3845

! red
*color1: #ff9233

! bright_red
*color9: #ffc999

! green
*color2: #33ff92

! bright_green
*color10: #99ffc9

! yellow
*color3: #a0ff33

! bright_yellow
*color11: #cfff99

! blue
*color4: #9233ff

! bright_blue
*color12: #c999ff

! magenta
*color5: #ff33a0

! bright_magenta
*color13: #ff99cf

! cyan
*color6: #33a0ff

! bright_cyan
*color14: #99cfff

! white
*color7: #bac7d4

! bright_white
*color15: #d9e5f2

! fonts
! run "fc-list" for a list of available fonts
URxvt*font: xft:Terminus:pixelsize=12
URxvt*scrollBar: False
URxvt*scrollTtyOutput: False
URxvt*scrollTtyKeypress: True
URxvt*secondaryScroll: True
URxvt*saveLines: 8000
URxvt*letterSpace: 0
URxvt*cursorBlink: True
URxvt*cursorUnderline: True
URxvt*modifier: alt
URxvt.perl-ext-common : default,matcher,tabbed,keyboard-select,url-select,clipboard
URxvt.urlLauncher     : firefox
URxvt.matcher.button  : 1
URxvt.tabbed.tabbar-fg: 5
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg:    14
URxvt.tabbed.tab-bg:    0
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-s: perl:keyboard-select:search
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.autocopy: True
URvxt.url-select.button: 2
URxvt.url-select.launcher: firefox
URxvt.url-select.underline: True
URxvt.keysym.M-c:   perl:clipboard:copy
URxvt.keysym.M-v:   perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped

